using System;

namespace Chapter13
{
    public class Program
    {
        public static void Main(string[] args)
        {
            Countdown(10);
        }

        static void Countdown(int n)
        {
            if (n == 1) Console.WriteLine(n);
            else
            {
                Console.WriteLine(n);
                Countdown(n - 1);
            }
        }
    }
}
